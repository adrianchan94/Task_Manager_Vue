,EAAE,CAAC,IAAI,CAAC;AAA3C,aAAuD,IAAjE;AAAA,OAAf,CAAb;AACD,KAZO;AAaR,IAAA,aAbQ,2BAaO;AACb,WAAK,WAAL,GAAmB,CAAC,KAAK,WAAzB;AACF;AAfQ,GAbG;AA8Bb,EAAA,OA9Ba,qBA8BH;AACR,SAAK,KAAL,GAAa,CACX;AACE,MAAA,EAAE,EAAE,CADN;AAEE,MAAA,IAAI,EAAE,iBAFR;AAGE,MAAA,GAAG,EAAE,qBAHP;AAIE,MAAA,QAAQ,EAAE;AAJZ,KADW,EAOX;AACE,MAAA,EAAE,EAAE,CADN;AAEE,MAAA,IAAI,EAAE,WAFR;AAGE,MAAA,GAAG,EAAE,qBAHP;AAIE,MAAA,QAAQ,EAAE;AAJZ,KAPW,EAaX;AACE,MAAA,EAAE,EAAE,CADN;AAEE,MAAA,IAAI,EAAE,iBAFR;AAGE,MAAA,GAAG,EAAE,sBAHP;AAIE,MAAA,QAAQ,EAAE;AAJZ,KAbW,CAAb;AAoBF;AAnDa,CAAf","sourcesContent":["<template>\n  <div class=\"container\">\n    <Header @toggle-add-task=\"toggleAddTask\" title=\"Task Tracker\"\n    :showAddTask=\"showAddTask\"\n    />\n    <div v-if=\"showAddTask\">\n    <AddTask @add-task=\"addTask\"/>\n    </div>\n    <Tasks @toggle-reminder=\"toggleReminder\" @delete-task=\"deleteTask\" :tasks=\"tasks\"/>\n  </div>\n</template>\n\n<script>\nimport Header from './components/Header'\nimport Tasks from './components/Tasks'\nimport AddTask from './components/AddTask'\n\nexport default {\n  name: 'App',\n  components: {\n    Header,\n    Tasks,\n    AddTask\n  },\n  data() {\n    return  {\n      tasks: [],\n      showAddTask: false\n    }\n  },\n  methods : {\n    addTask(task){\n      console.log(task)\n      this.tasks = [...this.tasks, task]\n    },\n    deleteTask(id) {\n      if(confirm('Are you sure want to delete this task?')) {\n        this.tasks = this.tasks.filter((task) => task.id !== id )\n      }\n    },\n    toggleReminder(id) {\n      this.tasks = this.tasks.map((task) => task.id === id ? {...task, reminder: !task.reminder} : task )\n    },\n    toggleAddTask(){\n      this.showAddTask = !this.showAddTask\n    }\n  },\n  created() {\n    this.tasks = [\n      {\n        id: 1,\n        text: 'Date with Heena',\n        day: 'March 5th at 2:30pm',\n        reminder: true,\n      },\n      {\n        id: 2,\n        text: 'Interview',\n        day: 'March 6th at 4:30pm',\n        reminder: true,\n      },\n      {\n        id: 3,\n        text: 'Lamma Saturdays',\n        day: 'March 10th at 1:30pm',\n        reminder: false,\n      },\n    ]\n  }\n}\n</script>\n\n<style>\n@import url('https://fonts.googleapis.com/css2?family=Poppins:wght@300;400&display=swap');\n* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n}\nbody {\n  font-family: 'Poppins', sans-serif;\n}\n.container {\n  max-width: 500px;\n  margin: 30px auto;\n  overflow: auto;\n  min-height: 300px;\n  border: 1px solid steelblue;\n  padding: 30px;\n  border-radius: 5px;\n}\n.btn {\n  display: inline-block;\n  background: #000;\n  color: #fff;\n  border: none;\n  padding: 10px 20px;\n  margin: 5px;\n  border-radius: 5px;\n  cursor: pointer;\n  text-decoration: none;\n  font-size: 15px;\n  font-family: inherit;\n}\n.btn:focus {\n  outline: none;\n}\n.btn:active {\n  transform: scale(0.98);\n}\n.btn-block {\n  display: block;\n  width: 100%;\n}\n</style>\n"],"sourceRoot":""}]}